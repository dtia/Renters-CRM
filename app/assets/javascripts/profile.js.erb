function getCreditScore() {
	var randCreditScore = getRandomCreditScore();
	$("#creditScore").html(randCreditScore);
	$.ajax({
		url: '/user/credit_check',
	    type: "POST",
	    data: {credit_score: randCreditScore},
	    success: function(resp){
			console.log('Credit score posted');
		}
	});
}

function getRandomCreditScore() {
	var randIncrement = Math.floor(Math.random()*101);
	var baseScore = 700;
	return baseScore + randIncrement;
}

function getBackgroundCheck() {
	$("#backgroundCheck").show();
}